  "bugs": {
    "web": "https://github.com/horosgrisa/asify/issues"
  },
  "repositories": [
    {
      "type": "git",
      "url": "https://github.com/horosgrisa/asify"
    }
  ],
  "licenses": [
    {
      "name": "MIT",
      "url": "https://github.com/horosgrisa/asify/LICENSE"
    }
  ],
  "homepage": "https://github.com/horosgrisa/asify"
}
